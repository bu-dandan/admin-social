table-column\n        prop=\"price\"\n        label=\"商品价格\">\n      </el-table-column>\n      <el-table-column\n        prop=\"type\"\n        label=\"商品分类\">\n      </el-table-column>\n      <el-table-column\n        label=\"操作\"\n        width=\"160\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"text\" size=\"small\" @click.native=\"clickFunc(2, scope.row.ID)\">收藏</el-button>\n          <el-button type=\"text\" size=\"small\" @click.native=\"clickFunc(3, scope.row.ID)\">加入购物车</el-button>\n          <el-button type=\"text\" size=\"small\" @click.native=\"clickFunc(4, scope.row.ID)\">购买</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    <el-pagination class=\"pagination\" background\n      @current-change=\"handleCurrentChange\"\n      :current-page.sync=\"page\"\n      :page-size=\"pageSize\"\n      layout=\"total, prev, pager, next\"\n      :total=\"total\">\n    </el-pagination>\n\n  </div>\n</template>\n\n<script>\nimport { getTableList, clickTable, searchTable } from \"@/api/table\"\n\nexport default {\n  name: 'index',\n  data() {\n    return {\n      input: '',\n      tableData: [],\n      page: 1,\n      pageSize: 10,\n      total: 7\n    }\n  },\n  mounted() {\n    this.getTableData()\n  },\n  methods: {\n    //获取表格数据，page，pagesize\n    getTableData() {\n      let that = this\n      let param = {\n        page: that.page,\n        pageSize: that.pageSize\n      }\n      // getTableList(param).then(res => {\n      //   console.log(res);\n      //   if (res.code != 200) return\n      //   let data = res.data.list\n      //   that.total = res.data.total\n      let data = [{\n        ID: '1',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '2',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '3',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '4',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '5',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '6',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }, {\n        ID: '7',\n        name: '商品名',\n        price: '100',\n        type: '服装',\n      }]\n\n      that.tableData = data\n      // })\n    },\n\n    search(input) {\n      let that = this\n      if (!input) {\n        this.$message({\n          message: '请先输入要搜索的商品',\n          type: 'warning'\n        });\n        return\n      }\n      let param = {\n        name: input\n      }\n      searchTable(param).then(res => {\n        let data = res.data\n        // let data = [{\n        //   ID: '1',\n        //   name: '商品名',\n        //   price: '100',\n        //   type: '服装',\n        // }]\n        that.tableData = data\n        that.total = 1\n      })\n    },\n\n    clickFunc(type, ID) {\n      let param = {\n        type: type,\n        ID: ID\n      }\n      let msg = type === 2 ? '商品已收藏'\n        : type === 3 ? '商品已成功加入购物车' : '商品购买成功'\n      // clickTable(param).then(res => {\n      // console.log(res);\n      // if (res.code != 200) {\n      //   this.$message({\n      //     message: '出错了',\n      //     type: 'error'\n      //   });\n      // }\n      this.$message({\n        message: msg,\n        type: 'success'\n      });\n      // })\n    },\n\n    //翻页功能\n    handleCurrentChange(page) {\n      this.page = page\n      this.getTableData()\n    },\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.index-container {\n  padding: 0 14px;\n}\n\n.search-box {\n  display: flex;\n  align-items: center;\n\n  padding-bottom: 32px;\n  border-bottom: 1px solid #EBEEF5;\n\n  .input {\n    margin-right: 14px;\n    width: auto !important;\n    min-width: 300px;\n  }\n}\n\n.pagination {\n  margin-top: 28px;\n  float: right;\n}\n</style>\n"]}]}